// 1013
[proc,xpdrops_setup_display_hoveroff](component $component0)
if (cc_find($component0, 0) = ^true) {
    cc_settrans(0);
    if (~on_mobile = false) {
        cc_setonmouseover("xpdrops_setup_display_hover(true, $component0)");
        cc_setonmouseleave("xpdrops_setup_display_hover(false, $component0)");
    }
}
if (cc_find($component0, 3) = ^true) {
    cc_settrans(0);
    cc_setgraphic("scrollbar_v2,1");
}
if (cc_find($component0, 4) = ^true) {
    cc_setcolour(0xff981f);
}

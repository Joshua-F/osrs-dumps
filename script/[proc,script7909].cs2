[proc,script7909](component $component0, int $int1, component $component2)
if (cc_find($component0, 1) = ^true) {
    if ($int1 = 1) {
        cc_setgraphic("options_radio_buttons,6");
    } else {
        cc_setgraphic("options_radio_buttons,5");
    }
    if_setonop("settings_side_new_renderer_toggle_op($component0, $int1, $component2)", $component0);
}

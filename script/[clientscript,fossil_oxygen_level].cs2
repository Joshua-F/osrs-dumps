// 1997
[clientscript,fossil_oxygen_level](component $component0, component $component1, component $component2)
if_setsize(interpolate(0, 16384, 0, 1000, %fossil_underwater_oxygen), 0, ^setsize_proportion, ^setsize_minus, $component0);
if (if_getwidth($component0) = 0 & %fossil_underwater_oxygen ! 0) {
    if_setsize(1, 0, ^setsize_abs, ^setsize_minus, $component0);
}
if (%fossil_underwater_oxygen = 0) {
    if_setsize(0, 0, ^setsize_abs, ^setsize_minus, $component0);
    return;
}
if_settext("Oxygen Level: <tostring(calc(%fossil_underwater_oxygen / 10))>%", $component1);
if (%fossil_underwater_oxygen <= 100 & %varclient_198 ! 1) {
    if_sethide(false, $component2);
    if_setontimer("fossil_warning_flasher($component2, $component1, clientclock, 250)", $component1);
}

// 8862
[proc,sailing_customisation_desc_font]()(graphic, fontmetrics)
def_graphic $graphic0 = p11_full;
def_fontmetrics $fontmetrics1 = p11_full;
switch_int (%responsive_current_breakpoint) {
    case 1 :
        $graphic0, $fontmetrics1 = p11_full, p11_full;
    case 2, 3 :
        $graphic0, $fontmetrics1 = p12_full, p12_full;
}
return($graphic0, $fontmetrics1);

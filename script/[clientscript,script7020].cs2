// 7020
[clientscript,script7020]
if (if_gettrans(muspah_overlay:tint_box) > %varclient_1143) {
    if_settrans(~max(50, calc(if_gettrans(muspah_overlay:tint_box) - 1)), muspah_overlay:tint_box);
} else if (if_gettrans(muspah_overlay:tint_box) < %varclient_1143) {
    if_settrans(~min(255, calc(if_gettrans(muspah_overlay:tint_box) + 1)), muspah_overlay:tint_box);
}

// 450
[clientscript,script450](component $component0, component $component1, int $int2)
if ($int2 = 1 | calc(clientclock % 100) = 0) {
    if (~high_risk_world = true) {
        if_settrans(0, $component0);
        if_settrans(100, $component1);
    } else {
        if_settrans(255, $component0);
        if_settrans(0, $component1);
    }
}

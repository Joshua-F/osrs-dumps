[proc,entity_highlight_active_obj](int $int0)
switch_int ($int0) {
    case 5, 9, 10, 11, 12 :
        %mouseover_obj_type, %mouseover_obj_coord = obj_type, obj_coord;
    case 4 :
        %interact_obj_type, %interact_obj_coord = obj_type, obj_coord;
    case default :
        return;
}
highlight_obj_on(obj_type, obj_coord, $int0, 0);

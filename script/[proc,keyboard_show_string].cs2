// 1838
[proc,keyboard_show_string](int $int0, int $int1, string $string0)
if (~on_mobile = false) {
    return;
}
%osm_last_keyboard = $int0;
%osm_last_keyboard_limit = $int1;
switch_int ($int0) {
    case 0 :
        mobile_keyboardshowstring($string0, $int1);
    case 1 :
        mobile_keyboardshowinteger($string0, $int1);
}

[proc,xptracker_get_id](stat $stat0)(int)
switch_stat ($stat0) {
    case null :
        return(%xptracker_position_overall);
    case attack :
        return(%xptracker_position_attack);
    case strength :
        return(%xptracker_position_strength);
    case defence :
        return(%xptracker_position_defence);

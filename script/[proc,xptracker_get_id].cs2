        return(%xptracker_position_runecraft);
    case construction :
        return(%xptracker_position_construction);
    case hitpoints :
        return(%xptracker_position_hitpoints);
    case agility :
        return(%xptracker_position_agility);
    case herblore :
        return(%xptracker_position_herblore);
    case thieving :
        return(%xptracker_position_thieving);
    case crafting :
        return(%xptracker_position_crafting);
    case fletching :
        return(%xptracker_position_fletching);
    case slayer :
        return(%xptracker_position_slayer);
    case hunter :
        return(%xptracker_position_hunter);
    case mining :
        return(%xptracker_position_mining);
    case smithing :
        return(%xptracker_position_smithing);

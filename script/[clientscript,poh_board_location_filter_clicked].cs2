// 3120
[clientscript,poh_board_location_filter_clicked](component $component0, component $component1, component $component2)
if (%poh_board_filtered = 1) {
    %poh_board_filtered = 0;
} else {
    %poh_board_filtered = 1;
}
~poh_board_location_filter_activate($component0, $component1, $component2);

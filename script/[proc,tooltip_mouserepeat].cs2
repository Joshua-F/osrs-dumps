// 837
[proc,tooltip_mouserepeat](component $component0, int $int1, component $component2, string $string0, int $int3, int $int4)
if (~on_enhanced_mobile = true) {
    return;
}
if (~on_enhanced_any = false & %tooltip_time < calc(clientclock + $int3)) {
    if (%tooltip_time < clientclock) {
        %tooltip_time = clientclock;
    }
    %tooltip_time = calc(%tooltip_time + 2);
    return;
}
%tooltip_time = calc(clientclock + $int3 + 10);
if (%tooltip_built = 0 & if_gethide($component2) = false) {
    ~tooltip_build($component0, $int1, $component2, $string0, $int4);
}

// 8676
[clientscript,sailing_menu_frame_click](component $component0, component $component1, component $component2, int $int3)
sound_synth(synth_2266, 1, 0);
if_sethide(true, $component1);
if_sethide(true, $component2);
if (cc_find($component0, 0) = ^true) {
    cc_setgraphic("menu_buttons,0");
}
if ($int3 >= 0) {
    %sailing_log_tab_opened = $int3;
    if_sethide(true, sailing_log:task_window);
    ~sailing_log_redraw;
}

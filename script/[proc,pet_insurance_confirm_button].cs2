.cc_create($component1, ^iftype_rectangle, 0);
.cc_setsize(0, 0, ^setsize_minus, ^setsize_minus);
.cc_setposition(0, 0, ^setpos_abs_centre, ^setpos_abs_centre);
.cc_setcolour(^black);
.cc_setfill(true);
.cc_settrans(255);
if (~on_mobile = false) {
    .cc_setonmouserepeat("script6133(1, $component2)");

// 5539
[proc,popout_tooltip_hook_if](component $component0)
if (~on_mobile = true) {
    if_setnoclickthrough(true, $component0);
    if_setonmouserepeat(null, $component0);
    if_setonmouseleave(null, $component0);
} else {
    if_setonmouseleave("popout_tooltip_clear_clientscript", $component0);
    if_setonclick(null, $component0);
    if_setnoclickthrough(false, $component0);
}

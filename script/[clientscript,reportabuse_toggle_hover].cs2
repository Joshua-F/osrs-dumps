// 5099
[clientscript,reportabuse_toggle_hover](int $int0, component $component1, int $int2)
if ($int0 = 1) {
    if (cc_find($component1, $int2) = ^true) {
        cc_settrans(220);
    }
    if_setontimer("reportabuse_toggle_reset(calc(clientclock + 2), event_com, $int2)", $component1);
} else {
    if (cc_find($component1, $int2) = ^true) {
        cc_settrans(255);
    }
    if_setontimer(null, $component1);
}

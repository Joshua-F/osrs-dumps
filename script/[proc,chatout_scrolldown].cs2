// 76
[proc,chatout_scrolldown]
def_string $string0 = null;
if (%chatout_readpos ! -1) {
    %chatout_readpos = calc(%chatout_readpos + 1);
    if (%chatout_readpos >= 20) {
        %chatout_readpos = 0;
    }
    if (%chatout_readpos = %chatout_writepos) {
        %chatinput = "";
        %chatout_readpos = -1;
    } else {
        $string0 = ~chatout_get(%chatout_readpos);
        if (string_length($string0) > 0) {
            %chatinput = $string0;
        }
    }
}

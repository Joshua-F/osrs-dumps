[proc,osm_entity_interaction_op]
if (~on_mobile = false) {
    return;
}
if (%settings_minimenu_filter_entity = 1) {
    if (%settings_mobile_tile_highlight_enabled = 1) {
        ~script7586(0);
        mes("Tile highlighting disabled!");
    } else {
        mes("Interactions enabled!");
    }
} else {
    mes("Interactions disabled!");
}
~osm_entity_interaction_set;

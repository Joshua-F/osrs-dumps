// 386
[proc,pvp_icons_layout]
if (~pvpw_check = true) {
    if (%varclient_53 = 1) {
        if_setposition(14, 0, interface_90:com1);
        if_setposition(14, 0, interface_90:com2);
    } else {
        if_setposition(14, 15, interface_90:com1);
        if_setposition(14, 15, interface_90:com2);
    }
    if_setposition(0, 26, interface_90:com3);
    if_setcolour(0xff981f, interface_90:com3);
    if_sethide(false, interface_90:com4);
    ~pvp_icons_comlevelrange(~wilderness_level);
} else {
    if_setposition(14, 13, interface_90:com1);
    if_setposition(14, 13, interface_90:com2);
    if_setposition(0, 39, interface_90:com3);
    if_setcolour(^yellow, interface_90:com3);
    if_sethide(true, interface_90:com4);
}

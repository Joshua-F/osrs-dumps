// 5172
[proc,script5172]
%helper_agility_next_level = calc(stat_base(agility) + 1);
def_int $int0 = calc(enum(int, int, enum_256, %helper_agility_next_level) - stat_visible_xp(agility));
if (%helper_agility_lap_xp ! 0) {
    %helper_agility_laps_until_level = calc($int0 / %helper_agility_lap_xp);
}

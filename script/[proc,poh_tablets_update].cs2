// 476
[proc,poh_tablets_update](component $component0, model $model1, model $model2, int $int3, int $int4)
if (%varplayer_261 >= $int3 & %varplayer_262 >= $int4) {
    if_setmodel($model2, $component0);
    if_setop(1, "Make-1", $component0);
    if_setop(2, "Make-5", $component0);
    if_setop(3, "Make-All", $component0);
    if_setop(4, "Make-X", $component0);
} else {
    if_setmodel($model1, $component0);
    if_clearops($component0);
}

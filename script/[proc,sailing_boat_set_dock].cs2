[proc,sailing_boat_set_dock](int $int0, int $int1)
switch_int ($int0) {
    case 1 :
        %sailing_boat_1_port = $int1;
    case 2 :
        %sailing_boat_2_port = $int1;
    case 3 :
        %sailing_boat_3_port = $int1;
    case 4 :
        %sailing_boat_4_port = $int1;
    case 5 :
        %sailing_boat_5_port = $int1;
    case default :
        ~script2075("Setting dock for invalid boat: <tostring($int0)>.");
}

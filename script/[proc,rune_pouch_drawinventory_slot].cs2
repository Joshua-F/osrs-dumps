// 671
[proc,rune_pouch_drawinventory_slot](obj $obj0, int $int1, int $int2)
cc_clearops;
cc_setontimer(null);
if ($obj0 ! null) {
    if (enum(obj, int, enum_55, $obj0) ! 0) {
        cc_settrans(0);
        cc_setobject($obj0, $int1);
        cc_setopbase("<col=ff9040><oc_name($obj0)></col>");
        switch_int (%rune_pouch_selectedquantity) {
            case default :
                cc_setop(1, "Store-1");
                cc_setop(3, "Store-5");
                if (%rune_pouch_customquantity > 0) {
                    cc_setop(4, "Store-<~tostring_spacer(%rune_pouch_customquantity, ",")>");
                }
                cc_setop(6, "Store-All");
            case 1 :
                cc_setop(1, "Store-5");
                cc_setop(2, "Store-1");
                if (%rune_pouch_customquantity > 0) {
                    cc_setop(4, "Store-<~tostring_spacer(%rune_pouch_customquantity, ",")>");
                }
                cc_setop(6, "Store-All");
            case 3 :
                if (%rune_pouch_customquantity > 0) {
                    cc_setop(1, "Store-<~tostring_spacer(%rune_pouch_customquantity, ",")>");
                } else {
                    cc_setop(1, "Store-X");
                }
                cc_setop(2, "Store-1");
                cc_setop(3, "Store-5");
                cc_setop(6, "Store-All");
            case 2 :
                cc_setop(1, "Store-All");
                cc_setop(2, "Store-1");
                cc_setop(3, "Store-5");
                if (%rune_pouch_customquantity > 0) {
                    cc_setop(4, "Store-<~tostring_spacer(%rune_pouch_customquantity, ",")>");
                }
        }
        cc_setop(5, "Store-X");
        cc_setop(10, "Examine");
        cc_setonop("cc_settrans_temporarily(event_com, event_comsubid, 100, 0)");
        cc_setondragcomplete("rune_pouch_inventoryreorder(event_com, event_com2, event_comsubid, event_comsubid2, 200, 0, $int2)");
    } else {
        cc_settrans(150);
        cc_setobject($obj0, $int1);
        cc_setopbase("<col=ff9040><oc_name($obj0)></col>");
        cc_setop(1, "Store");
        cc_setop(10, "Examine");
        cc_setonop("cc_settrans_temporarily(event_com, event_comsubid, 175, 150)");
        cc_setondragcomplete("rune_pouch_inventoryreorder(event_com, event_com2, event_comsubid, event_comsubid2, 200, 150, $int2)");
    }
} else {
    cc_settrans(0);
    cc_setobject(blankobject, 1);
    cc_setonop(null);
    cc_setondragcomplete(null);
}

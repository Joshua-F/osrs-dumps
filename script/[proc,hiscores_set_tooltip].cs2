[proc,hiscores_set_tooltip](string $string0, int $int0, int $int1)
if (~on_mobile = false) {
    cc_setonmouserepeat("tooltip_build_progress_bar(event_com, event_comsubid, interface_894:com_2, $string0, $int0, $int1)");
    cc_setonmouseleave("deltooltip(interface_894:com_2)");
} else {

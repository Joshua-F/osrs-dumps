[clientscript,gauntlet_timer_end]
def_component $component0 = gauntlet_overlay:timer_text;
if_setontimer(null, gauntlet_overlay:timer);
if_setsize(0, if_getheight($component0), ^setsize_minus, ^setsize_abs, $component0);
if_setposition(0, if_gety($component0), ^setpos_abs_centre, ^setpos_abs_top, $component0);
if_settextalign(^settextalign_centre, ^settextalign_centre, 0, $component0);
if_settext("Final Encounter", $component0);
if_setcolour(0xfcfce7, $component0);

// 701
[proc,emote_update](component $component0)
def_int $int1 = 0;
while ($int1 <= 43) {
    if (cc_find($component0, $int1) = ^true) {
        if (~emote_checkunlocked($int1) = true) {
            cc_setgraphic(enum(int, graphic, enum_1001, $int1));
        } else {
            cc_setgraphic(enum(int, graphic, enum_1002, $int1));
        }
    }
    $int1 = calc($int1 + 1);
}

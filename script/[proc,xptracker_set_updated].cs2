// 5391
[proc,xptracker_set_updated](stat $stat0, boolean $boolean1)
switch_stat ($stat0) {
    case null :
        %xptracker_updated_overall = $boolean1;
    case attack :
        %xptracker_updated_attack = $boolean1;
    case strength :
        %xptracker_updated_strength = $boolean1;
    case defence :
        %xptracker_updated_defence = $boolean1;
    case ranged :
        %xptracker_updated_ranged = $boolean1;
    case prayer :
        %xptracker_updated_prayer = $boolean1;
    case magic :
        %xptracker_updated_magic = $boolean1;
    case runecraft :
        %xptracker_updated_runecraft = $boolean1;
    case construction :
        %xptracker_updated_construction = $boolean1;
    case hitpoints :
        %xptracker_updated_hitpoints = $boolean1;
    case agility :
        %xptracker_updated_agility = $boolean1;
    case herblore :
        %xptracker_updated_herblore = $boolean1;
    case thieving :
        %xptracker_updated_thieving = $boolean1;
    case crafting :
        %xptracker_updated_crafting = $boolean1;
    case fletching :
        %xptracker_updated_fletching = $boolean1;
    case slayer :
        %xptracker_updated_slayer = $boolean1;
    case hunter :
        %xptracker_updated_hunter = $boolean1;
    case mining :
        %xptracker_updated_mining = $boolean1;
    case smithing :
        %xptracker_updated_smithing = $boolean1;
    case fishing :
        %xptracker_updated_fishing = $boolean1;
    case cooking :
        %xptracker_updated_cooking = $boolean1;
    case firemaking :
        %xptracker_updated_firemaking = $boolean1;
    case woodcutting :
        %xptracker_updated_woodcutting = $boolean1;
    case farming :
        %xptracker_updated_farming = $boolean1;
}

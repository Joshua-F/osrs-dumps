// 5951
[proc,entity_highlight_active_npc](int $int0)
switch_int ($int0) {
    case 5, 9, 10, 11, 12 :
        %mouseover_npc_uid, %mouseover_npc_creationcycle = npc_uid, npc_creationcycle;
    case 4 :
        %interact_npc_uid, %interact_npc_creationcycle = npc_uid, npc_creationcycle;
    case default :
        return;
}
highlight_npc_on(npc_uid, npc_creationcycle, $int0);

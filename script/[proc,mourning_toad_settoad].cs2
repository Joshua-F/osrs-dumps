// 1054
[proc,mourning_toad_settoad](component $component0)
if (%mourning_gun_ammo = 1) {
    if_setmodel(model_29323, $component0);
} else if (%mourning_gun_ammo = 2) {
    if_setmodel(model_3447, $component0);
} else if (%mourning_gun_ammo = 3) {
    if_setmodel(model_29322, $component0);
} else if (%mourning_gun_ammo = 4) {
    if_setmodel(model_29324, $component0);
} else {
    if_setmodel(model_16238, $component0);
}

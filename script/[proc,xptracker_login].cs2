// 5450
[proc,xptracker_login](int $int0, string $string0, string $string1)
if ($int0 >= 60 | compare($string0, %xptracker_player_name) ! 0 | compare($string1, %xptracker_world_type) ! 0) {
    %xptracker_needs_var_reset = 1;
}
%xptracker_player_name = $string0;
%xptracker_world_type = $string1;
%xptracker_has_checked_login = 1;

// 3063
[clientscript,bees_init](component $component0, component $component1)
if_setontimer("bees($component0, calc(random(6) - 3), calc(random(6) - 3), $component1, 0)", $component0);
if_setonmouseover("bees_mouseover($component0)", $component0);
if_setonmouserepeat("bees_mouserepeat($component0, event_mousex, event_mousey)", $component0);
if_setonmouseleave("bees_mouseleave($component0)", $component0);

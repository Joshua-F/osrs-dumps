[proc,settings_slider_choose_bobble](int $int0, int $int1, component $component2, graphic $graphic3)
switch_int ($int0) {
    case 30, 31, 32 :
        if (~on_enhanced_any = false) {
            cc_setgraphic($graphic3);
            return;
        }
        if (%option_master_volume <= 0) {
            cc_setgraphic("settings_slider,12");

// 5393
[proc,xptracker_set_timetrained](stat $stat0, int $int1)
switch_stat ($stat0) {
    case null :
        %xptracker_timetrained_overall = $int1;
    case attack :
        %xptracker_timetrained_attack = $int1;
    case strength :
        %xptracker_timetrained_strength = $int1;
    case defence :
        %xptracker_timetrained_defence = $int1;
    case ranged :
        %xptracker_timetrained_ranged = $int1;
    case prayer :
        %xptracker_timetrained_prayer = $int1;
    case magic :
        %xptracker_timetrained_magic = $int1;
    case runecraft :
        %xptracker_timetrained_runecraft = $int1;
    case construction :
        %xptracker_timetrained_construction = $int1;
    case hitpoints :
        %xptracker_timetrained_hitpoints = $int1;
    case agility :
        %xptracker_timetrained_agility = $int1;
    case herblore :
        %xptracker_timetrained_herblore = $int1;
    case thieving :
        %xptracker_timetrained_thieving = $int1;
    case crafting :
        %xptracker_timetrained_crafting = $int1;
    case fletching :
        %xptracker_timetrained_fletching = $int1;
    case slayer :
        %xptracker_timetrained_slayer = $int1;
    case hunter :
        %xptracker_timetrained_hunter = $int1;
    case mining :
        %xptracker_timetrained_mining = $int1;
    case smithing :
        %xptracker_timetrained_smithing = $int1;
    case fishing :
        %xptracker_timetrained_fishing = $int1;
    case cooking :
        %xptracker_timetrained_cooking = $int1;
    case firemaking :
        %xptracker_timetrained_firemaking = $int1;
    case woodcutting :
        %xptracker_timetrained_woodcutting = $int1;
    case farming :
        %xptracker_timetrained_farming = $int1;
}

// 7905
[clientscript,settings_side_new_renderer_toggle_mouserepeat](component $component0, component $component1)
def_string $string0 = "Use new renderer (currently off)<br>When active, the new renderer will be used.";
if (%settings_hd_new_renderer_toggle = 1) {
    $string0 = "Use new renderer (currently on)<br>When active, the new renderer will be used.";
}
if (~on_mobile = false & cc_find($component0, 0) = ^true) {
    cc_setcolour(^white);
}
~tooltip_mouserepeat($component0, -1, $component1, $string0, 25, 190);

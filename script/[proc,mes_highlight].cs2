// 8962
[proc,mes_highlight](int $int0)(string)
switch_int ($int0) {
    case 1 :
        return(~mes_highlight_red);
    case 2 :
        return(~mes_highlight_green);
    case 3 :
        return(~mes_highlight_purple);
    case 4 :
        return(~mes_highlight_magenta);
    case 5 :
        return(~mes_highlight_blue);
    case 6 :
        return(~mes_highlight_cyan);
    case 7 :
        return(~mes_highlight_orange);
    case default :
        return("");
}

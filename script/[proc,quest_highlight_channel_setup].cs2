[proc,quest_highlight_channel_setup]
if (%quest_start_highlight = 0) {
    highlight_npctype_setup(13, null, 1, 0, 0);
    highlight_loctype_setup(16, null, 1, 0, 0);
} else {
    highlight_npctype_setup(13, ^cyan, 2, 30, 1);
    highlight_loctype_setup(16, ^cyan, 2, 30, 1);
    ~quest_highlight_entity_setup;
}

[clientscript,zeah_overlay_shrink_it](component $component0)
def_int $int1 = if_getwidth($component0);
$int1 = calc($int1 - 5);
if ($int1 < 120) {
    $int1 = 120;
}
def_int $int2 = if_getheight($component0);
$int2 = calc($int2 - 2);
if ($int2 < 60) {
    $int2 = 60;
}
if_setsize($int1, $int2, ^setsize_abs, ^setsize_abs, $component0);
if ($int1 = 120 & $int2 = 60) {
    if_setontimer(null, $component0);
}

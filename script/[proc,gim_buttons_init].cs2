// 5252
[proc,gim_buttons_init](component $component0, component $component1, int $int2)
def_int $rgb3 = 0x66ff00;
def_int $rgb4 = 0xff2020;
if ($int2 = 1) {
    ~gim_button_init($component0, "Confirm", $rgb3, false);
    ~gim_button_init($component1, "Regular Account", $rgb4, false);
} else if ($int2 = 2) {
    ~gim_button_init($component1, "Confirm", $rgb3, false);
    ~gim_button_init($component0, "GIM Account", $rgb4, false);
} else {
    ~gim_button_init($component1, "Regular Account", $rgb4, false);
    ~gim_button_init($component0, "GIM Account", $rgb4, false);
}

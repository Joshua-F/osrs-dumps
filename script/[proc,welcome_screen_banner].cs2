// 3091
[proc,welcome_screen_banner]
if (clienttype ! ^clienttype_ios) {
    if_setonmouserepeat("comp_sethide(false, interface_413:com81)", interface_413:com78);
    if_setonmouseleave("comp_sethide(true, interface_413:com81)", interface_413:com78);
    if_setop(1, "View news", interface_413:com78);
} else {
    if_setop(1, "", interface_413:com78);
}
if_sethide(true, interface_413:com81);

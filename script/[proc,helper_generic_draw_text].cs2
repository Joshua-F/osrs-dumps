def_int $int2 = calc($int1 * 14);
if (cc_find(helper_generic:content, $int0) = ^true) {
    cc_setsize(%helper_generic_current_width, $int2, ^setsize_abs, ^setsize_abs);
    cc_setposition(1, %helper_generic_current_height, ^setpos_abs_left, ^setpos_abs_top);
    cc_setcolour(^white);

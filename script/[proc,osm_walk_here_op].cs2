// 7435
[proc,osm_walk_here_op]
if (~on_mobile = false) {
    return;
}
if (%settings_minimenu_filter_walk = 1) {
    if (%settings_mobile_tile_highlight_enabled = 1) {
        ~script7586(0);
        mes("Tile highlighting disabled!");
    } else {
        mes("Walk enabled!");
    }
} else {
    mes("Walk disabled!");
}
~osm_walk_here_set;

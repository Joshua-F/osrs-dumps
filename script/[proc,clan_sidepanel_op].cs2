// 4401
[proc,clan_sidepanel_op](int $int0, int $int1, string $string0, int $int2, int $int3)
switch_int ($int0) {
    case 6 :
        return;
    case 7 :
        if ($int2 ! -1) {
            if ($int3 = 2 & activeclanchannel_getuserrank($int2) > -1) {
                return;
            }
            if ($int3 = 0) {
                ~chat_send("Attempting to kick player from clan chat...", 3, $int3, 0, -1);
            }
            activeclanchannel_kickuser($int2);
        } else {
            mes("That player appears to have left the channel.");
        }
    case 8 :
        if (activeclansettings_find_affined($int3) = true) {
            if ($int3 = 2) {
                return;
            }
            if ($int2 ! -1) {
                if ($int3 = 0) {
                    ~chat_send("Attempting to ban player from clan chat...", 3, $int3, 0, -1);
                }
                affinedclansettings_addbanned_fromchannel($int2, $int3);
            } else {
                mes("That player appears to have left the channel.");
            }
        } else {
            mes("The game cannot process your Ban request now.");
        }
    case 9 :
        if ($int1 = 1) {
            friend_del($string0);
        } else {
            friend_add($string0);
        }
    case 10 :
        if ($int1 = 2) {
            ignore_del($string0);
        } else {
            ignore_add($string0);
        }
}

[clientscript,script6148](int $int0, int $int1)
if (cc_find(pet_insurance:pets, $int0) = ^true) {
    if (cc_gettrans = 210) {
        return;
    }
    if (calc($int1 % 2) = 0) {
        cc_settrans(245);
    } else {
        cc_settrans(235);
    }
    if (.cc_find(pet_insurance:pets, calc($int0 + 1)) = ^true) {
        .cc_setoutline(1);
    }
    if (.cc_find(pet_insurance:pets, calc($int0 + 2)) = ^true) {
        .cc_setcolour(0x9f9f9f);
    }
}

// 928
[clientscript,smokeoverlay_init](component $component0)
cc_deleteall($component0);
cc_create($component0, ^iftype_rectangle, 0);
cc_setsize(0, interpolate(0, 5, 0, 20, 16384), ^setsize_minus, ^setsize_rel);
cc_setposition(0, 0, ^setpos_abs_centre, ^setpos_abs_bottom);
cc_setfill(true);
if_setmodeltransparent(true, smokeoverlay:haze);
cc_setcolour(^black);
cc_setblendcolour(0x808080);
cc_settrans(255);
cc_setblendtrans(64);
cc_setblendmode(^blendmode_vgrad_trans);

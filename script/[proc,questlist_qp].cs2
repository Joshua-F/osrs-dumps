// 1356
[proc,questlist_qp](component $component0)
def_string $string0 = ",";
if (~speedrunning_world = false) {
    if_settext("Quest Points: <~tostring_spacer(%qp, $string0)>/<~tostring_spacer(%qp_max, $string0)>", $component0);
} else {
    if_settext("Speedrun Trophies: <~tostring_spacer(~speedrunning_total_trophies, $string0)>", $component0);
}

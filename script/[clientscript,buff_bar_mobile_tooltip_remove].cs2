// 3171
[clientscript,buff_bar_mobile_tooltip_remove](component $component0, int $int1, component $component2)
if (%buff_bar_tooltip = -1) {
    if_setontimer(null, $component2);
    return;
}
if (clientclock < $int1) {
    return;
}
~deltooltip_action($component0);
%buff_bar_tooltip = -1;
if_setontimer(null, $component2);

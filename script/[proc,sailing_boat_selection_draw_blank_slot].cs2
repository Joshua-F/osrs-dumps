        case 3 :
            cc_settext("Unlocks at level <tostring(50)> Sailing");
        case 4 :
            cc_settext("Unlocks at level <tostring(78)> Sailing");
        case default :
            cc_settext("Unlocks at level <tostring(91)> Sailing");

// 8629
[proc,sailing_boat_selection_draw_blank_slot](component $component0, int $int1, int $int2, int $int3, int $int4, int $int5, int $int6)(int, int)
cc_create($component0, ^iftype_rectangle, $int1);
cc_setposition(0, $int4, ^setpos_abs_centre, ^setpos_abs_top);
cc_setcolour(^black);
cc_setfill(true);
cc_setsize(0, $int5, ^setsize_minus, ^setsize_abs);
if (calc($int6 % 2) = 1) {
    cc_settrans(200);
} else {
    cc_settrans(220);
}
$int1 = calc($int1 + 1);
cc_create($component0, ^iftype_text, $int1);
if ($int3 = 0) {
    switch_int ($int2) {

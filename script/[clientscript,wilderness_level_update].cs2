[clientscript,wilderness_level_update]
def_int $int0 = coordz(coord);
def_int $int1 = calc(($int0 - 9920) / 8 + 1);
def_int $int2 = calc(($int0 - 3520) / 8 + 1);
if ($int0 > 6400) {
    if_settext("Level: <tostring($int1)>", interface_381:com1);
} else {
    if_settext("Level: <tostring($int2)>", interface_381:com1);
}

[universe]
type=layer
widthmode=minus
heightmode=minus
xmode=abs_centre
ymode=abs_centre
onload=lumbridge_alchemy_init(event_com, lumbridge_alchemy:low, lumbridge_alchemy:high, lumbridge_alchemy:title, lumbridge_alchemy:items, lumbridge_alchemy:charges)

[low]
type=graphic
width=24
height=24
layer=universe
graphic="magicoff,10"
events=2
op1=Low Alchemy

[high]
type=graphic
x=24
width=24
height=24
layer=universe
graphic="magicoff,26"
events=2
op1=High Alchemy

[title]
type=text
height=22
widthmode=minus
xmode=abs_centre
layer=universe
textfont=b12_full
textalignh=1
textalignv=1
textshadow=yes
colour=0xff981f

[close]
type=graphic
x=3
y=3
width=16
height=16
xmode=abs_right
layer=universe
graphic="v2_stone_close_button,0"
events=2
op1=Close
onmouseover=graphic_swapper(event_com, "v2_stone_close_button,1")
onmouseleave=graphic_swapper(event_com, "v2_stone_close_button,0")
onop=opsound(event_op, 2)

[frame]
type=layer
y=22
width=172
height=228
xmode=abs_centre
layer=universe
onload=thinstonebox(event_com)

[com_6]
type=graphic
width=2
height=2
widthmode=minus
heightmode=minus
xmode=abs_centre
ymode=abs_centre
layer=frame
graphic=tradebacking_light
tiling=yes

[items]
type=layer
width=4
height=4
widthmode=minus
heightmode=minus
xmode=abs_centre
ymode=abs_centre
layer=frame

[charges]
type=text
height=250
widthmode=minus
heightmode=minus
xmode=abs_centre
ymode=abs_bottom
layer=universe
textfont=p11_full
textalignh=1
textalignv=2
textshadow=yes
colour=0xff981f

